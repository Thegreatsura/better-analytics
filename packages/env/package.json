{
	"name": "@better-analytics/env",
	"version": "0.0.0",
	"private": true,
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.js",
			"source": "./src/index.ts"
		},
		"./*": {
			"types": "./dist/*.d.ts",
			"import": "./dist/*.js",
			"require": "./dist/*.js",
			"source": "./src/*.ts"
		}
	},
	"scripts": {
		"build": "tsc --build --clean && tsc"
	},
	"dependencies": {
		"dotenv": "^16.5.0",
		"@types/node": "^24.0.3"
	},
	"devDependencies": {
		"@better-analytics/typescript": "workspace:*"
	}
}
